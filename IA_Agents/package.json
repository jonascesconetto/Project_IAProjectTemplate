{
  "name": "ai-project-template",
  "version": "1.0.0",
  "description": "Template completo para projetos de agentes de IA - cobrindo Produto, Gestão, Arquitetura, Organização e Custo",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:eval": "jest tests/evaluation/",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "cost:daily": "node scripts/cost-report.js --period=daily",
    "cost:weekly": "node scripts/cost-report.js --period=weekly",
    "cost:monthly": "node scripts/cost-report.js --period=monthly",
    "evaluate": "node scripts/evaluate.js",
    "deploy:check": "node scripts/deploy-check.js",
    "init": "node scripts/init-project.js"
  },
  "keywords": [
    "ai",
    "agent",
    "template",
    "claude",
    "anthropic",
    "llm",
    "multi-agent",
    "finops"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
